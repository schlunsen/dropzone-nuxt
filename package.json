{
  "name": "dropzone-nuxt",
  "version": "0.1.3",
  "description": "A Dropzone.js module for Nuxt.js",
  "author": {
    "name": "Rasmus Schl√ºnsen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schlunsen/dropzone-nuxt.git"
  },
  "keywords": [
    "vue",
    "dropzone",
    "nuxt",
    "nuxt.js"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write"
  },
  "devDependencies": {
    "jest": "^26.0.1",
    "nuxt": "^2.4.0",
    "prettier": "^1.16.1",
    "puppeteer": "^1.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "consola": "^2.3.2",
    "vue2-dropzone": "^3.6.0"
  }
}
